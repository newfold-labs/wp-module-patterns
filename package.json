{
	"name": "@newfold-labs/wp-module-patterns",
	"version": "0.1.0",
	"description": "Wonder Blocks",
	"license": "GPL-2.0-or-later",
	"private": true,
	"author": {
		"name": "Hristina",
		"email": "hristina.conjic@newfold.com"
	},
	"scripts": {
		"build": "concurrently \"wp-scripts build ./src/wonder-blocks.js\" \"npm run utils:prod\"",
		"format": "wp-scripts format ./src",
		"start": "concurrently \"wp-scripts start ./src/wonder-blocks.js\" \"npm run utils:dev\" \"npm run json-server\"",
		"lint:js": "wp-scripts lint-js ./src",
		"lint:js:fix": "wp-scripts lint-js ./src --fix",
		"utils:dev": "postcss ./css-utilities/utilities.css -o ./assets/styles/utilities.css -w --env development",
		"utils:prod": "postcss ./css-utilities/utilities.css -o ./assets/styles/utilities.css --env production",
		"json-server": "json-server --watch api.dev.json --port 3003"
	},
	"devDependencies": {
		"@wordpress/eslint-plugin": "^14.0.0",
		"@wordpress/prettier-config": "^2.10.0",
		"@wordpress/scripts": "^25.4.0",
		"autoprefixer": "^10.4.13",
		"cssnano": "^5.1.15",
		"json-server": "^0.17.3",
		"postcss": "^8.4.21",
		"postcss-cli": "^10.1.0",
		"postcss-import": "^15.1.0",
		"postcss-safe-important": "^1.2.1",
		"prettier-plugin-tailwindcss": "^0.2.3",
		"tailwindcss": "^3.2.7",
		"webpack-merge": "^5.8.0"
	},
	"dependencies": {
		"@wordpress/icons": "^9.18.0",
		"classnames": "^2.3.2",
		"concurrently": "^7.6.0",
		"lodash": "^4.17.21",
		"react-masonry-css": "^1.0.16",
		"swr": "^2.0.3"
	}
}
